{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "dev": {
        "cache": false
      },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["apps/web/.next/**", "!apps/web/.next/cache/**", "apps/datafetcher/dist/*"]
    },
    "deploy": {
      "dependsOn": ["build"],
      "outputs": []
    }
  },
  "globalEnv": ["RABBITMQ_USERNAME", "RABBITMQ_PASSWORD", "RABBITMQ_HOST"]
}
